{% extends 'base.njk' %}

{%- block contents %}
            {% if items|length %}
            <p>Items defining this concept:</p>
            <ul>
            {%- for i in items %}
            <li><a href="{{ i.permalink|url }}">{{ i.id }}</a></li>
            {%- endfor %}
            </ul>
            {% else %}
            <p>No items define this concept</p>
            {% endif %}
{%- endblock %}
