{% extends 'base/base.njk' %}

{% block pagetitle %}Sources{% endblock %}

{%- block contents %}
<main>
    <h2>Sources</h2>
    <ul>
    {%- for i in items %}
    <li>
        <a href="{{ i.permalink | url }}">{{ i.name }}</a><br>
        {{ i.title }}
    </li>
    {%- endfor %}
    </ul>
</main>
{%- endblock %}
